#get
	padding 0 0 50px
.get-list
	width 100%
	max-width 560px
	margin 0 auto 40px
	li
		position relative
		list-style none
		padding-left: 12px
		margin-bottom: 15px
		&:before
			content: ''
			display block
			position: absolute
			left 0
			top: 9px
			border-radius: 50px
			background-color: bl
			width 7px
			height 7px
		&:last-child
			margin-bottom: 0
.get-text
	text-align: center
	margin-top: 15px
	font-size: 20px
	span
		font-family: RoBo
		color bl
		display block
		font-size: 22px